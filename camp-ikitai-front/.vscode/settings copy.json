{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "\\.java$",
        "cmd": "java -jar ./lib/google-java-format-1.10.0-all-deps.jar --replace ${file}"
      }
    ]
  },
  "java.format.enabled": false,
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "asciidoc.use_asciidoctor_js": false,
  "asciidoc.preview.refreshInterval": 500,
  "asciidoc.preview.useEditorStyle": false,
  "asciidoc.asciidoctor_command": "asciidoctor  -a outdir=tmp -a imagesdir=tmp -a imagesoutdir=tmp -a icons=font -r asciidoctor-diagram -o-"
}